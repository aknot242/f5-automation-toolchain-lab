{
    "$schema": "https://raw.githubusercontent.com/F5Networks/f5-telemetry-streaming/master/src/nodejs/schema/base_schema.json",
    "class": "Telemetry",
    "controls": {
        "class": "Controls",
        "logLevel": "info"
    },
    "My_Poller": {
        "class": "Telemetry_System_Poller",
        "interval": 60
    },
    "My_Listener": {
        "class": "Telemetry_Listener",
        "port": 6514
    },
    "Kafka_Consumer": {
        "class": "Telemetry_Consumer",
        "type": "Kafka",
        "host": "{{ubuntuDockerHost}}",
        "protocol": "binaryTcp",
        "port": {{kafkaConsumerPort}},
        "topic": "f5-telemetry"
    },
    "Statsd_Consumer": {
        "class": "Telemetry_Consumer",  
        "type": "Statsd",
        "host": "{{ubuntuDockerHost}}",
        "protocol": "udp",
        "port": {{statsdConsumerPort}}
    },
    "ElasticSearch_Consumer": {
        "class": "Telemetry_Consumer",
        "type": "ElasticSearch",
        "host": "{{ubuntuDockerHost}}",
        "index": "f5telemetryindex",
        "port": {{elasticsearcConsumerPort}},
        "protocol": "http",
        "dataType": "_doc"
    }
}
